@model Demo
<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="fw-bold py-3 mb-4">
        <span class="text-muted fw-light">Experencies/</span> Deteil / @Model.Id 
        <span>
            <a class="btn btn-primary active w-100" href="/Admin/Experencies/Index">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-box-arrow-left" viewBox="0 0 16 16">
                    <path fill-rule="evenodd" d="M6 12.5a.5.5 0 0 0 .5.5h8a.5.5 0 0 0 .5-.5v-9a.5.5 0 0 0-.5-.5h-8a.5.5 0 0 0-.5.5v2a.5.5 0 0 1-1 0v-2A1.5 1.5 0 0 1 6.5 2h8A1.5 1.5 0 0 1 16 3.5v9a1.5 1.5 0 0 1-1.5 1.5h-8A1.5 1.5 0 0 1 5 12.5v-2a.5.5 0 0 1 1 0v2z" />
                    <path fill-rule="evenodd" d="M.146 8.354a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L1.707 7.5H10.5a.5.5 0 0 1 0 1H1.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3z" />
                </svg> Back
            </a>
        </span>
    </h4>


    <div class="row">
        <!-- Vertical Scrollbar -->
        <div class="col-md-6 col-sm-12">
            <div class="card overflow-hidden mb-4" style="height: 300px">
                <h5 class="card-header">Content </h5>
                <div class="card-body ps ps--active-y" id="vertical-example">
                    <p>
                        @Model.Description
                    </p>
                    <div class="ps__rail-x" style="left: 0px; bottom: 0px;">
                        <div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div>
                    </div>
                    <div class="ps__rail-y" style="top: 0px; height: 232px; right: 0px;">
                        <div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 45px;"></div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ Vertical Scrollbar -->
        <!-- Horizontal Scrollbar -->
        <div class="col-md-6 col-sm-12">
            <div class="card overflow-hidden mb-4" style="height: 300px">
                <h5 class="card-header">File</h5>
                <div class="card-body ps ps--active-x" id="horizontal-example">
                    <div class="row">
                        @if (Model.Input != "")
                        {
                            <div class="col-6">
                                <iframe src="@Model.Input" frameborder="0"></iframe>
                            </div>
                            <div class="col-6">

                                <a href="@Model.Input" target="_blank" class="link-dark">
                                    <button type="button" class="btn btn-primary d-block active" autocomplete="off"
                                        aria-pressed="true">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                                         class="bi bi-box-arrow-up-right" viewBox="0 0 16 16">
                                            <path fill-rule="evenodd"
                                              d="M8.636 3.5a.5.5 0 0 0-.5-.5H1.5A1.5 1.5 0 0 0 0 4.5v10A1.5 1.5 0 0 0 1.5 16h10a1.5 1.5 0 0 0 1.5-1.5V7.864a.5.5 0 0 0-1 0V14.5a.5.5 0 0 1-.5.5h-10a.5.5 0 0 1-.5-.5v-10a.5.5 0 0 1 .5-.5h6.636a.5.5 0 0 0 .5-.5z" />
                                            <path fill-rule="evenodd"
                                              d="M16 .5a.5.5 0 0 0-.5-.5h-5a.5.5 0 0 0 0 1h3.793L6.146 9.146a.5.5 0 1 0 .708.708L15 1.707V5.5a.5.5 0 0 0 1 0v-5z" />
                                        </svg> Look
                                    </button>
                                </a>
                            </div>
                        }
                        else
                        {
                            <div class="container d-flex align-items-center justify-content-center">
                                <iframe src="https://www.animatedimages.org/data/media/942/animated-anime-image-0199.gif"
                                    frameborder="0"></iframe>
                            </div>
                            <div class="alert alert-primary" role="alert">File empty </div>
                        }
                    </div>
                    <div class="ps__rail-x" style="width: 681px; left: 0px; bottom: 0px;">
                        <div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 236px;"></div>
                    </div>
                    <div class="ps__rail-y" style="top: 0px; right: 0px;">
                        <div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 0px;"></div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ Horizontal Scrollbar -->
        <!-- Vertical & Horizontal Scrollbars -->
        <div class="col-12">
            <div class="card overflow-hidden" style="height: 500px">
                <h5 class="card-header">@Model.Name - Details</h5>
                <div class="card-body ps ps--active-x ps--active-y" id="both-scrollbars-example">
                    <div class="row">
                        <div class="col-6">
                            <b>
                                Date Time
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                     fill="currentColor" class="bi bi-calendar-day" viewBox="0 0 16 16">
                                    <path d="M4.684 11.523v-2.3h2.261v-.61H4.684V6.801h2.464v-.61H4v5.332h.684zm3.296 0h.676V8.98c0-.554.227-1.007.953-1.007.125 0 .258.004.329.015v-.613a1.806 1.806 0 0 0-.254-.02c-.582 0-.891.32-1.012.567h-.02v-.504H7.98v4.105zm2.805-5.093c0 .238.192.425.43.425a.428.428 0 1 0 0-.855.426.426 0 0 0-.43.43zm.094 5.093h.672V7.418h-.672v4.105z" />
                                    <path d="M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5zM1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4H1z" />
                                </svg> :
                            </b> <span>@Model.StartingDate</span>
                        </div>

                        <div class="col-6 end">
                            <b>
                                End Time
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                     fill="currentColor" class="bi bi-calendar-day" viewBox="0 0 16 16">
                                    <path d="M4.684 11.523v-2.3h2.261v-.61H4.684V6.801h2.464v-.61H4v5.332h.684zm3.296 0h.676V8.98c0-.554.227-1.007.953-1.007.125 0 .258.004.329.015v-.613a1.806 1.806 0 0 0-.254-.02c-.582 0-.891.32-1.012.567h-.02v-.504H7.98v4.105zm2.805-5.093c0 .238.192.425.43.425a.428.428 0 1 0 0-.855.426.426 0 0 0-.43.43zm.094 5.093h.672V7.418h-.672v4.105z" />
                                    <path d="M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5zM1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4H1z" />
                                </svg> :
                            </b> <span>@Model.EndingDate</span>
                        </div>
                        <hr>
                        <div class="col-6">
                            @if (Model.Url != null)
                            {
                                <div class="mb-3">
                                    <label class="form-label">Url</label>
                                    <input class="form-control" type="text" placeholder="@Model.Url" readonly="">
                                </div>
                            }
                            else
                            {
                                <div class="alert alert-primary w-100" role="alert">File empty </div>
                                <div class="mb-3">
                                    <iframe src="https://www.animatedimages.org/data/media/942/animated-anime-image-0051.gif"
                                        frameborder="0" style="border: none;" scrolling="no"></iframe>
                                </div>
                            }
                        </div>
                    </div>
                    <div class="ps__rail-x" style="width: 1388px; left: 0px; bottom: 0px;">
                        <div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 980px;"></div>
                    </div>
                    <div class="ps__rail-y" style="top: 0px; height: 432px; right: 0px;">
                        <div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 143px;"></div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ Vertical & Horizontal Scrollbars -->
    </div>
</div>

